  },
  "homepage": "https://github.com/Welcome-Bot/discord-pagination#readme",
  "devDependencies": {
    "@types/node": "^16.4.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "discord.js": "^13.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
